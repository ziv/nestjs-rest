{
  "name": "std-json-api",
  "version": "0.0.0",
  "description": "JSON-API standard utilities for NestJS",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig-lib.json",
    "test": "tsx --test",
    "release": "semantic-release"
  },
  "author": "Ziv Perry",
  "license": "MIT",
  "keywords": [
    "json",
    "api",
    "json-api",
    "specification",
    "standard"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziv/nestjs-rest.git",
    "directory": "packages/std-json-api"
  },
  "bugs": {
    "url": "https://github.com/ziv/nestjs-rest/issues"
  },
  "homepage": "https://github.com/ziv/nestjs-rest#readme",
  "files": [
    "*.js",
    "*.d.ts",
    "readme.md",
    "license"
  ],
  "release": {
    "tagFormat": "std-json-api-v${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "qs": ">=6.14.1"
  }
}
